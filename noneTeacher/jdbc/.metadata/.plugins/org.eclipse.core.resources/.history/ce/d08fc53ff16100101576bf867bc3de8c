package myjdbc;

public class InheritanceTest {
	public static void main(String[] args) {
		// 객체생성
		Ractangle ractangle = new Ractangle();
		ractangle.print();
	}
	//	Shape shape = new Shape();  객체생성 불가
	Shape shape;
}

// 일종의 추상클래스, .class로 존재한다. 객체생성되지 않는다. 그러나 타입으로는 생성가능하다.
// 내부에 메서드를 선언하는 용도로 사용한다.
interface Shape {
	//define method(정의)는 불가 
	// declare method(선언)은 가능, 추상메서드라고도 한다. 바디 x, 호출 x
	public abstract void print(); // 선언은 가능, public abstract는 없어도 자동으로 적용됨
}

class Ractangle {
	// 정의란 {} 구간을 만드는 것이고 호출되었을 때 무엇이 실행될지를 기술해 둔다.
	void print() {
		System.out.println("Rectangle");
	}
}