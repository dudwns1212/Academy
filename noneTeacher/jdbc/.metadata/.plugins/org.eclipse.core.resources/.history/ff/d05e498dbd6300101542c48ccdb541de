package project;
// 직원정보를 담은(변수정의) 클래스
public class EmpInfo {
	public int employee_id;
	public String last_name;
	public String first_name;
	public int salary;
	public String email;
	public String hireDate;
	
	public String toString() {
		return "EmpInfo [직원번호=" + employee_id + ", 성=" + last_name + ", 명=" + first_name + "salary=" + salary + ", email="
				+ email + ", hire_date=" + hireDate + "]";
	}
}

