package Ex03;

public class CarTest {
	public static void main(String[] args) {
		// Car 객체 생성
		Car hudson = new Car();
		Car carrera = new Car();
		Car mater = new Car();
		
		hudson.getName("Hudson");
		carrera.getName("Carrera");
		mater.getName("Mater");
		
		hudson.getSpeed(300);
		mater.getSpeed(80);
		carrera.getSpeed(200);
		
		System.out.println("차 이름 : " + hudson.setName() + " 차 스피드 : " + hudson.setSpeed());
		
		
	}
}

class Car {
	
	String name;
	int speed;
	
	public void setName() {
		this.name=name;
	}
	public void setSpeed() {
		this.speed=speed;
	}
	public String getName(String name) {
		return name;
	}
	public int getSpeed(int speed) {
		return speed;
	}
	
}
